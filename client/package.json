{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.4",
    "@mui/system": "5.11.4",
    "axios": "1.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dotenv": "0.1.3",
    "react-router-dom": "6.6.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "NODE_ENV=prod react-dotenv && react-scripts start",
    "dev": "NODE_ENV=dev react-dotenv && react-scripts start",
    "test": "DEBUG_PRINT_LIMIT=30000 react-scripts test --watchAll --verbose",
    "test:coverage": "react-scripts test --coverage --watchAll --verbose",
    "build": "react-dotenv && react-scripts build",
    "eject": "react-scripts eject"
  },
  "homepage": ".",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "eslint": "8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "react-test-renderer": "18.2.0"
  },
  "_comment": "See https://stackoverflow.com/questions/73958968/cannot-use-import-statement-outside-a-module-with-axios",
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs",
      "react-dotenv": "<rootDir>/__mocks__/react-dotenv.js"
    }
  },
  "react-dotenv": {
    "whitelist": [
      "NODE_ENV",
      "BASE_URL",
      "BASE_URL_TEST"
    ]
  }
}
